<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_food_gray"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_margin="@dimen/dimen_10_dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_48_dp"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/dimen_15_dp"
            android:text="商家从业资质"
            android:textColor="@color/text_333333"
            android:textSize="@dimen/dimen_14_dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/view_e5e5e5" />

        <com.gxuc.runfast.shop.view.ZFlowLayout
            android:id="@+id/ll_img_contain"
            android:layout_marginLeft="@dimen/dimen_10_dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_10_dp" />

    </LinearLayout>

</LinearLayout>